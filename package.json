{
  "name": "chemistry",
  "version": "0.1.0",
  "description": "Objects that React!",
  "license": "MIT",
  "author": "Doug Rubino",
  "main": "dist/chemistry.cjs",
  "module": "dist/chemistry.js",
  "types": "dist/chemistry.d.ts",
  "exports": {
    ".": {
      "types": "./dist/chemistry.d.ts",
      "import": "./dist/chemistry.js",
      "require": "./dist/chemistry.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npm run build && npx serve site",
    "lint": "next lint",
    "compile:c": "rollup -c",
    "build:c": "npm run clean:c && npm run compile:c",
    "watch:c": "rollup -c -w",
    "clean:c": "rm -rf dist",
    "publish:c": "npm publish",
    "prepublishOnly": "npm run build:c",
    "publish:dry": "npm pack --dry-run"
  },
  "dependencies": {
    "fast-safe-stringify": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "rollup": "^4.50.2",
    "rollup-plugin-dts": "^6.2.3",
    "tailwindcss": "^4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.22",
    "marked": "^16.1.2",
    "next": "15.4.5",
    "styled-components": "^6.1.19"
  }
}
